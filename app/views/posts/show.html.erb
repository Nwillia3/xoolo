<p id="notice"><%= notice %></p>

<p>
  <strong>Headline:</strong>
  <%= @post.headline %>
</p>

<p>
  <strong>Questionone:</strong>
  <%= @post.questionone %>
</p>

<p>
  <strong>Descriptionone:</strong>
  <%= @post.descriptionone %>
</p>

<p>
  <strong>Questiontwo:</strong>
  <%= @post.questiontwo %>
</p>

<p>
  <strong>Descriptiontwo:</strong>
  <%= @post.descriptiontwo %>
</p>

<p>
  <strong>Questionthree:</strong>
  <%= @post.questionthree %>
</p>

<p>
  <strong>Descriptionthree:</strong>
  <%= @post.descriptionthree %>
</p>

<p class="date">
  Submitted <%= time_ago_in_words(@post.created_at) %> Ago
</p>
<% if user_signed_in? %>
  <%= link_to 'Edit', edit_post_path(@post) %> 
<% end %>
<%= link_to 'Back', posts_path %>

<div id="comments">
  <h2><%= @post.comments.count %> Comments</h2>
  <%= render @post.comments %>

  <h3>Add a comment:</h3>
  <%= render "comments/form" %>
</div>
