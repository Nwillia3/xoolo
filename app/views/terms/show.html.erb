<div id="content">
	
	<div class="row">
		<div class="Post">
			<p id="notice"><%= notice %></p>


			<h1 id="Morning">Finance term of the day</h1>

			<h3> <%= @term.title%> </h3>

			<p> <%= @term.definition %> </p>

			<% if user_signed_in? %>
				<%= link_to 'Edit', edit_term_path(@term) %> 
			<% end %> 

			<%= link_to 'Back', terms_path %>

		</div>
	</div>
</div>









